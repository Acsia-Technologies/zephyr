#	Hence chose FS32K144HAT0MLLT which closely resembles the required target SOC


if BOARD_S32K144_Q100_EVAL

config BOARD
	default "s32k144_q100_eval"

if CAN

config GPIO
	default y

endif # CAN


endif # BOARD_S32K144_Q100_EVAL
